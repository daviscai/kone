{
  "name": "kone",
  "version": "1.0.0",
  "description": "kone is a framework base on Koa2 for node.js",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc  ava --reporter=text-lcov",
    "ava": "./node_modules/.bin/ava -v -w test/",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "build": "./node_modules/.bin/rollup -c rollup.config.js",
    "build-min": "NODE_ENV=production ./node_modules/.bin/rollup -c rollup.config.js",
    "dev": "./node_modules/.bin/nodemon ./index.js",
    "start": "node ./pm2.js",
    "compile": "./node_modules/.bin/babel --copy-files src/ --out-dir app/",
    "watch": "./node_modules/.bin/babel --copy-files  src/ --out-dir app/ --watch"
  },
  "keywords": [
    "koa2",
    "kone",
    "node.js framework"
  ],
  "author": "caiwxiong@qq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daviscai/kone.git"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2"
      ]
    },
    "files": [
      "test/**/*spec.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "antd": "^2.6.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.7",
    "consolidate": "^0.14.5",
    "content-disposition": "^0.5.2",
    "content-type": "^1.0.2",
    "cookies": "0.6.2",
    "csrf": "^3.0.4",
    "fast-safe-stringify": "^1.1.11",
    "formidable": "^1.1.1",
    "fresh": "^0.4.0",
    "helmet": "^3.4.0",
    "http2": "^3.3.6",
    "i18n": "^0.8.3",
    "ioredis": "^2.5.0",
    "mime-types": "^2.1.14",
    "mongoose": "^4.8.6",
    "mysql": "^2.12.0",
    "mz": "^2.6.0",
    "nunjucks": "^3.0.0",
    "on-finished": "^2.3.0",
    "parseurl": "^1.3.1",
    "range-parser": "^1.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "resolve-path": "^1.3.3",
    "sequelize": "^3.30.2",
    "statuses": "^1.3.1",
    "type-is": "^1.6.14",
    "uid-safe": "^2.1.3",
    "vary": "^1.1.0",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "codecov": "^1.0.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "pm2": "^2.3.0",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      [
        "stage-2"
      ],
      [
        "react"
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
