{
  "name": "kwan",
  "version": "0.0.2",
  "description": "kwan is a framework base on Koa2 for node.js",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc  ava --reporter=text-lcov",
    "ava": "./node_modules/.bin/ava -v -w test/",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "build": "./node_modules/.bin/rollup -c rollup.config.js",
    "build-min": "NODE_ENV=production ./node_modules/.bin/rollup -c rollup.config.js",
    "start-dev": "./node_modules/.bin/nodemon ./index.js",
    "start": "./node_modules/.bin/pm2 start ./index.js --watch ",
    "compile": "./node_modules/.bin/babel --copy-files --ignore config/locales/ src/ --out-dir app/ ; cp -R src/config/locales app/config/locales",
    "watch": "./node_modules/.bin/babel --copy-files --ignore config/locales/ src/ --out-dir app/ --watch"
  },
  "keywords": [
    "koa2",
    "kwan",
    "node.js framework"
  ],
  "author": "caiwxiong@qq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daviscai/kwan.git"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2"
      ]
    },
    "files": [
      "test/**/*spec.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "dependencies": {
    "antd": "^2.6.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "ioredis": "^2.0.0-rc4",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.4",
    "koa-i18n": "^1.2.0",
    "koa-jsonp": "^2.0.2",
    "koa-locale": "^1.2.0",
    "koa-log4": "^2.1.0",
    "koa-router": "^7.0.1",
    "koa-session-minimal": "^3.0.3",
    "koa-static2": "^0.1.8",
    "koa-views": "^5.2.0",
    "mysql": "^2.12.0",
    "nunjucks": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "sequelize": "^3.29.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "codecov": "^1.0.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "pm2": "^2.3.0",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "supertest": "^2.0.1"
  },
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      [
        "stage-2"
      ],
      [
        "react"
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
