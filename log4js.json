{
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "clustered",
      "appenders": [
        {
          "category": "http",
          "type": "logLevelFilter",
          "level": "WARN",
          "maxLevel": "FATAL",
          "appender": {
              "type": "dateFile",
              "filename": "error.log",
              "pattern": "-yyyy-MM-dd.bak"
          }
        }
      ]
    }
  ]
}
